Sub addMenu()
'addMyMenu Macro
'This macro will insert a menubar into the current workbook to allow easy a=
ccess to the
'necessary macros

    Dim myMenuBar As CommandBar
    Dim newMenu As CommandBarPopup, subMenu As CommandBarPopup
    Dim menuButton As CommandBarButton
    Dim menus As Integer
    Dim calcSetting As Integer

    Call delMenu

    Application.StatusBar =3D False

    Set myMenuBar =3D CommandBars("Worksheet Menu Bar")
    menus =3D CommandBars("Worksheet Menu Bar").Controls.Count

    Set newMenu =3D myMenuBar.Controls.Add(Type:=3DmsoControlPopup) ', befo=
re:=3Dmenus)
    newMenu.Caption =3D "&Personal Macros"
    newMenu.TooltipText =3D "Personal Macros"

    Set menuButton =3D newMenu.Controls.Add(Type:=3DmsoControlButton, ID:=
=3D1)
    menuButton.Caption =3D "Format &Percentage"
    menuButton.OnAction =3D "Percentage"

    Application.StatusBar =3D True

    Set menuButton =3D newMenu.Controls.Add(Type:=3DmsoControlButton, ID:=
=3D1)
    menuButton.Caption =3D "Switch Reference Style"
    menuButton.OnAction =3D "SwitchRefStyle"

    Set menuButton =3D newMenu.Controls.Add(Type:=3DmsoControlButton, ID:=
=3D1)
    menuButton.Caption =3D "Calculate Range"
    menuButton.OnAction =3D "CalculateRange"

    Set menuButton =3D newMenu.Controls.Add(Type:=3DmsoControlButton, ID:=
=3D1)
    menuButton.Caption =3D "Rebuild Calculation Chain"
    menuButton.OnAction =3D "RebuildCalculationSequence"

    Set menuButton =3D newMenu.Controls.Add(Type:=3DmsoControlButton, ID:=
=3D1)
    menuButton.Caption =3D "Unhide All Sheets"
    menuButton.OnAction =3D "UnhideAllSheets"

    Set menuButton =3D newMenu.Controls.Add(Type:=3DmsoControlButton, ID:=
=3D1)
    menuButton.Caption =3D "&No Auto Format Pivots"
    menuButton.OnAction =3D "NoAutoFormatPivots"

    Set menuButton =3D newMenu.Controls.Add(Type:=3DmsoControlButton, ID:=
=3D1)
    menuButton.Caption =3D "Format &Table"
    menuButton.OnAction =3D "FormatTable"

    Set menuButton =3D newMenu.Controls.Add(Type:=3DmsoControlButton, ID:=
=3D1)
    menuButton.Caption =3D "Format Table No Total Row"
    menuButton.OnAction =3D "FormatTableNoTotal"

    Set menuButton =3D newMenu.Controls.Add(Type:=3DmsoControlButton, ID:=
=3D1)
    menuButton.Caption =3D "Format MultiColumn &Heading"
    menuButton.OnAction =3D "Format_MultiColumn_Heading"

    Set menuButton =3D newMenu.Controls.Add(Type:=3DmsoControlButton, ID:=
=3D1)
    menuButton.Caption =3D "Pull &Color Theme"
    menuButton.OnAction =3D "PullColorTheme"

    Application.StatusBar =3D True

End Sub

Sub delMenu()
'delMyMenu Macro
    On Error Resume Next
    CommandBars("Worksheet Menu Bar").Controls("Personal Macros").Delete
End Sub



Sub BaseFormatCode(IncludeTotal As Boolean)

Dim rngSelection As Range

Set rngSelection =3D Selection

Dim IsLeftAlign As VbMsgBoxResult

IsLeftAlign =3D MsgBox("Left Align Cells?", vbYesNo, "Alignment")

    rngSelection.Borders(xlDiagonalDown).LineStyle =3D xlNone
    rngSelection.Borders(xlDiagonalUp).LineStyle =3D xlNone
    With rngSelection.Borders(xlEdgeLeft)
        .LineStyle =3D xlContinuous
        .ColorIndex =3D xlAutomatic
        .TintAndShade =3D 0
        .Weight =3D xlMedium
    End With
    With rngSelection.Borders(xlEdgeTop)
        .LineStyle =3D xlContinuous
        .ColorIndex =3D xlAutomatic
        .TintAndShade =3D 0
        .Weight =3D xlMedium
    End With
    With rngSelection.Borders(xlEdgeBottom)
        .LineStyle =3D xlContinuous
        .ColorIndex =3D xlAutomatic
        .TintAndShade =3D 0
        .Weight =3D xlMedium
    End With
    With rngSelection.Borders(xlEdgeRight)
        .LineStyle =3D xlContinuous
        .ColorIndex =3D xlAutomatic
        .TintAndShade =3D 0
        .Weight =3D xlMedium
    End With
    With rngSelection.Borders(xlInsideVertical)
        .LineStyle =3D xlContinuous
        .ColorIndex =3D xlAutomatic
        .TintAndShade =3D 0
        .Weight =3D xlThin
    End With
    With rngSelection.Borders(xlInsideHorizontal)
        .LineStyle =3D xlDashDotDot
        .ColorIndex =3D xlAutomatic
        .TintAndShade =3D 0
        .Weight =3D xlThin
    End With

    With Range(rngSelection(1, 1), rngSelection(1, rngSelection.Columns.Cou=
nt))
        With .Borders(xlEdgeBottom)
            .LineStyle =3D xlDouble
            .ColorIndex =3D xlAutomatic
            .TintAndShade =3D 0
            .Weight =3D xlThick
        End With

        With rngSelection
            .HorizontalAlignment =3D xlCenter
            .VerticalAlignment =3D xlCenter
            .WrapText =3D True
            .Orientation =3D 0
            .AddIndent =3D False
            .IndentLevel =3D 0
            .ShrinkToFit =3D False
            .ReadingOrder =3D xlContext
            .MergeCells =3D False
        End With
        .Borders(xlInsideHorizontal).LineStyle =3D xlNone


        If IsLeftAlign =3D vbYes Then
            With Range(rngSelection(2, 1), rngSelection(rngSelection.Rows.C=
ount, rngSelection.Columns.Count))
                .HorizontalAlignment =3D xlLeft
            End With
        End If


    End With

    If IncludeTotal Then

        With Range(rngSelection(rngSelection.Rows.Count, 1), rngSelection(r=
ngSelection.Rows.Count, rngSelection.Columns.Count))
            With .Borders(xlEdgeTop)
                .LineStyle =3D xlContinuous
                .ColorIndex =3D xlAutomatic
                .TintAndShade =3D 0
                .Weight =3D xlThin
            End With
        End With

    End If

    'With Range(rngSelection(1, 1), rngSelection(rngSelection.Rows.Count, 1=
))
    '    .HorizontalAlignment =3D xlCenter
    '    .VerticalAlignment =3D xlCenter
    '    .Orientation =3D 0
    '    .AddIndent =3D False
    '    .IndentLevel =3D 0
    '    .ShrinkToFit =3D False
    '    .ReadingOrder =3D xlContext
    '    .MergeCells =3D False
    'End With


End Sub



